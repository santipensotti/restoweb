@page "/mesasegura/{restaurantId}/{tableId}"
@inject HttpClient Http

<h3>Mesa @tableId - Restaurante @restaurantId</h3>

<div class="alert alert-success">
    Ubicación verificada. ¡Bienvenido!
</div>

@code {
    [Parameter]
    public string restaurantId { get; set; }
    
    [Parameter]
    public string tableId { get; set; }
}
